{
  "name": "apollo-quickstart",
  "version": "0.0.1",
  "author": "Tyler Knipfer <knipferrc@gmail.com>",
  "description": "Apollo and typescript starter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knipferrc/apollo-quickstart.git"
  },
  "bugs": {
    "url": "https://github.com/knipferrc/apollo-quickstart/issues"
  },
  "keywords": [
    "apollo",
    "graphql",
    "typescript",
    "api"
  ],
  "scripts": {
    "dev": "nodemon --require dotenv/config ./src/main.ts",
    "start": "node --require dotenv/config dist/main",
    "build": "tsc",
    "format": "prettier --write \"**/*.{tsx,ts,css,scss,md,js,json,html}\""
  },
  "devDependencies": {
    "@types/graphql": "^14.2.0",
    "dotenv": "^7.0.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "apollo-server": "^2.4.8",
    "graphql": "^14.2.0"
  }
}
